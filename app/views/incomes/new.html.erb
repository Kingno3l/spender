<section class='form-category-container'>
  <div class='form-category-content'>
    <%= form_with(model: @income, local: true, class: 'category-form') do |form| %>
      <div class='category-field'>
        <%= form.label :amount %>
        <%= form.number_field :amount %>
      </div>
    
    
      <div class='category-field submit-food-btn'>
        <%= form.submit "Proceed", class: 'submit-food' %>
      </div>
    <% end %>
  </div>
</section>